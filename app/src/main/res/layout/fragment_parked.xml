<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ParkedFragment" >

    <TextView
        android:id="@+id/ParkedInSpaceText"
        android:layout_width="253dp"
        android:layout_height="61dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="76dp"
        android:text="Parked In Space"
        android:textSize="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/SpaceNumberText"
        android:layout_width="156dp"
        android:layout_height="166dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="128dp"
        android:text="18"
        android:textSize="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ParkedInSpaceText" />

    <TextView
        android:id="@+id/LotInText"
        android:layout_width="218dp"
        android:layout_height="42dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="76dp"
        android:text="North Lot"
        android:textSize="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SpaceNumberText" />

    <Button
        android:id="@+id/LeaveSpacebutton"
        android:layout_width="235dp"
        android:layout_height="57dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="120dp"
        android:text="Leave Space"
        android:textSize="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>